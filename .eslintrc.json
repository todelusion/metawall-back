{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": [
    "standard-with-typescript",
    "airbnb-base",
    "airbnb-typescript/base",
    "prettier"
  ],
  "overrides": [],
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module",
    "project": "./tsconfig.json"
  },
  "plugins": ["prettier"],
  "rules": {
    "linebreak-style": "off",
    "@typescript-eslint/no-misused-promises": "off",
    "import/prefer-default-export": "off",
    "func-names": "off"
  }
}
